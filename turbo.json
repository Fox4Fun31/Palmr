{
  "$schema": "./node_modules/turbo/schema.json",
  "tasks": {
    "dev": {
      "cache": false,
      "persistent": true,
      "interactive": true
    },
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        ".next/**"
      ]
    },
    "lint": {
      "dependsOn": [
        "^build"
      ]
    },
    "lint:fix": {
      "dependsOn": [
        "^build"
      ],
      "cache": false
    },
    "format": {
      "cache": false
    },
    "format:check": {},
    "type-check": {
      "dependsOn": [
        "^build"
      ]
    },
    "validate": {
      "dependsOn": [
        "^build"
      ]
    }
  }
}